    "precommit": "lint-staged && jest",
    "prepare": "husky install",
    "prepublishOnly": "babel src -d dist",
    "test": "jest"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsaunders/prefix-loader.git"
  },
  "files": [
    "dist"
  ],
